# 문자 포함 여부
"Hello" in a    <<  a.contains("Hello")

# 인덱스
a.indexOf()
indexOf(String s)
indexOf(int i)
indexOf(String s, int startindex)
indexOf(int i, int startindex)

a.lastIndexOf()

lastIndexOf(String s)
lastIndexOf(int i)
lastIndexOf(String s, int startindex)
lastIndexOf(int i, int startindex)

# 문자열 비교시 equals or equalsIgnoreCase



File a = new File("파일명")
try{
	Scanner sc = new Scanner(File a);
	while (sc.hasNextInt()){       
		System.out.println(sc.nextInt());
	}
catch(FileNotFoundException e){
	파일을 불러올 때 오류가 발생 한 경우
	}
	
hasNextInt << 파일에서 입력 받아온 자료형이 정수형인 경우 true 아닌경우 false

# 추상 클래스 
외부 클래스의 public < 부분을 abstract로 바꾼다.
abstract class play{
	abstract void player();
}
public class Main extends play{
	public static void main(String[] args){
		Main main = new Main();
		main.player();

		void player{
			System.out.print("hello");
		}		
	}	
}


# 인터 페이스
추상 클래스와 비슷한 느낌
public interface play{
	abstract void player();
	public void player();

}
public class Main implements play{
	public static void(String[] args){
		Main main = new Main();
		main.player();
		main.player2();

		public void player{
			실행문
		}
		public void player2{
			실행문
		}
	}
}



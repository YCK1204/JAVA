"Hello" in a    <<  a.contains("Hello")

# 문자열 비교시 equals or equalsIgnoreCase

input() >> Scanner a = new Scanner(System.in)

File a = new File("파일명")
try{
	Scanner sc = new Scanner(File a);
	while (sc.hasNextInt()){       hasNextInt << 파일에서 입력 받아온 자료형이 정수형인 경우 true 아닌경우 false
		System.out.println(sc.nextInt());
	}
catch(FileNotFoundException e){
	파일을 불러올 때 오류가 발생 한 경우
	}

# 삼항 연산자
(a > b) ? true : false
